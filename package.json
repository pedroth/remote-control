{
  "name": "remote-control",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "bun run ./server.mjs",
    "pub": "bun version patch; git push origin $(git describe --tags `git rev-list --tags --max-count=1`)",
    "build-windows": "bun build --compile --target=bun-windows-x64 ./server.mjs --outfile remote-control-server.exe",
    "build-linux": "bun build --compile --target=bun-linux-x64 ./server.mjs --outfile remote-control-server"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@jitsi/robotjs": "^0.6.11",
    "express": "^4.18.2",
    "ip": "^1.1.8",
    "open": "^9.1.0"
  }
}